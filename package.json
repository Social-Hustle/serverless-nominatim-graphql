{
  "name": "serverless-nominatim-graphql",
  "version": "1.0.0",
  "description": "A Serverless API boilerplate using AWS Lambda, GraphQL and Typescript",
  "main": "src/graphql.ts",
  "license": "MIT",
  "scripts": {
    "schema-type": "graphql-codegen --config codegen.json",
    "build": "npm run schema-type && tsc",
    "start": "sls offline",
    "deploy": "npm run build && serverless deploy",
    "test": "jest"
  },
  "dependencies": {
    "apollo-server-lambda": "^2.19.0",
    "graphql": "^15.4.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.8.1",
    "@graphql-codegen/typescript": "^2.7.1",
    "@graphql-codegen/typescript-type-graphql": "^2.3.1",
    "@types/jest": "^28.1.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "aws-sdk": "^2.789.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^29.0.1",
    "prettier": "^2.1.2",
    "serverless-offline": "^6.8.0",
    "ts-jest": "^28.0.8",
    "typescript": "^4.0.5"
  }
}
